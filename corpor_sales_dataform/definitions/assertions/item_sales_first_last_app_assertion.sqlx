WHERE NOT (
    (item_nbr = 10 AND item_last_sales_app_past_7_days = 0) OR
    (item_nbr = 20 AND item_last_sales_app_past_7_days = 1) OR
    (item_nbr = 10 AND item_percent_days_with_sales_last_7_days = 3.0 / 7) OR
    (item_nbr = 20 AND item_percent_days_with_sales_last_7_days = 2.0 / 7) OR
    (item_nbr = 20 AND item_last_sales_app_past_7_days is NULL)
)